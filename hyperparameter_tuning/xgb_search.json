{
    "hyperparameters": {
        "type": "object",
        "properties": {
            "n_estimators": {
                "type": "number",
                "min": 1000,
                "max": 2000,
                "scaling": "logarithmic"
            },
            "max_depth": {
                "type": "number",
                "min": 1,
                "max": 32,
                "scaling": "logarithmic"
            },
            "eta": {
                "type": "number",
                "min": 0.1,
                "max": 0.5,
                "scaling": "logarithmic"
            },
            "subsample": {
                "type": "number",
                "min": 0.5,
                "max": 1.0,
                "scaling": "logarithmic"
            },
            "alpha": {
                "type": "number",
                "min": 0,
                "max": 20,
                "scaling": "logarithmic"
            }
        }
    },
    "function": {
        "type": "python_function",
        "module": "modelxgb",
        "name": "trainxgb"
    },
    "search": {
        "method": "atpe",
        "iterations": 1000
    },
    "results": {
        "directory": "resultsxgb",
        "graphs": true
    },
    "ui": {
        "enabled": true
    }
}